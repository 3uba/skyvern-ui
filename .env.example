# Skyvern backend — internal URL (server-side only, never exposed to browser)
SKYVERN_INTERNAL_URL=http://skyvern:8000

# Skyvern artifact server (port 9090) — serves file:// screenshots/recordings
# Defaults to same host as SKYVERN_INTERNAL_URL on port 9090
# SKYVERN_ARTIFACT_URL=http://skyvern-ui:9090

# PostgreSQL — connection string for the skyvern_ui database
DATABASE_URL=postgresql://skyvern:skyvern@localhost:5432/skyvern_ui

# Better Auth — session encryption secret (generate with: openssl rand -base64 32)
BETTER_AUTH_SECRET=change-me-in-production

# App URLs
BETTER_AUTH_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
